# file Makefile
# make file for CrdMsdcUsg card job handler, release msdccmbd_zedboard
# author Alexander Wirthmueller
# date created: 12 Sep 2018
# modified: 12 Sep 2018

include ../Makefile.inc

CPP = /opt/Xilinx/petalinux-v2017.2/tools/linux-i386/gcc-arm-linux-gnueabi/bin/arm-linux-gnueabihf-g++
CPPFLAGS = --sysroot=/home/mpsitech/emb/zedboard/avnet-digilent-zedboard-2017.2/build/tmp/sysroots/plnx_arm -std=c++11 -mfloat-abi=hard -g -Wall -Os -pthread -DMSDCCMBD -Wno-unknown-pragmas

STATLIB = /opt/Xilinx/petalinux-v2017.2/tools/linux-i386/gcc-arm-linux-gnueabi/bin/arm-linux-gnueabihf-ar
STATLIBFLAGS = rcs

INCPATH = -I=$(SRCROOT)/dbsmsdc -I=$(SRCROOT)/msdccmbd/VecMsdc -I=$(SRCROOT)/msdccmbd/IexMsdc -I=$(SRCROOT)/msdccmbd
INCPATH += -I=$(UASDKROOT)/SDK_Source/interface/common/platform_specific/atomic/any_platform_st -I=$(UASDKROOT)/SDK_Source/interface/common/platform_specific/file/linux_windows -I=$(UASDKROOT)/SDK_Source/interface/common/platform_specific/library/linux_windows -I=$(UASDKROOT)/SDK_Source/interface/common/platform_specific/locks/any_platform_st -I=$(UASDKROOT)/SDK_Source/interface/common/platform_specific/ref_count/any_platform_st -I=$(UASDKROOT)/SDK_Source/interface/common/platform_specific/tcpip/windows_linux_sockets_select_st -I=$(UASDKROOT)/SDK_Source/interface/common/platform_specific/thread_pool/any_platform_st -I=$(UASDKROOT)/SDK_Source/interface/common/platform_specific/timers/any_platform_polled -I=$(UASDKROOT)/SDK_Source/interface/common/portable/address_space -I=$(UASDKROOT)/SDK_Source/interface/common/portable/address_space/helpers -I=$(UASDKROOT)/SDK_Source/interface/common/portable/data_types -I=$(UASDKROOT)/SDK_Source/interface/common/portable/data_types/helper -I=$(UASDKROOT)/SDK_Source/interface/common/portable/miscellaneous -I=$(UASDKROOT)/SDK_Source/interface/common/portable/smart_pointers -I=$(UASDKROOT)/SDK_Source/interface/common/portable/ua_messages -I=$(UASDKROOT)/SDK_Source/interface/server/platform_specific/server_class/windows_linux_st -I=$(UASDKROOT)/SDK_Source/interface/server/portable -I=$(UASDKROOT)/SDK_Source/internals -I=/home/root/src/devmsdd -I=/usr/include -I=/usr/include/libxml2 -I=/usr/include/spinnaker -I=/usr/local/include

OBJS = CrdMsdcUsg.o PnlMsdcUsgRec.o PnlMsdcUsgDetail.o PnlMsdcUsgAAccess.o QryMsdcUsgAAccess.o PnlMsdcUsgMNUser.o QryMsdcUsgMNUser.o PnlMsdcUsgHeadbar.o PnlMsdcUsgList.o QryMsdcUsgList.o

LIB = libcrdmsdcusg

.SUFFIXES: .cpp

.cpp.o :
	$(CPP) $(CPPFLAGS) $(INCPATH) -o $@ -c $<

all:	$(LIB)

$(LIB):	$(OBJS)
	$(STATLIB) $(STATLIBFLAGS) $(LIB).a $(OBJS)
	rm -f *.o

install:
	mv $(LIB).a /home/mpsitech/emb/zedboard/avnet-digilent-zedboard-2017.2/build/tmp/sysroots/plnx_arm$(LIBROOT)/msdccmbd/

clean:
	rm -f *.o

